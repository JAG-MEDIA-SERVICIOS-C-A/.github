{
  "ignorePatterns": [
    {
      "pattern": "^https://img.shields.io"
    },
    {
      "pattern": "^../../../"
    },
    {
      "pattern": "^https://github.com/.+/(issues|pulls)$"
    }
  ],
  "replacementPatterns": [
    {
      "pattern": "^/",
      "replacement": "{{BASEURL}}/"
    }
  ],
  "timeout": "20s",
  "retryOn429": true,
  "retryCount": 5,
  "fallbackRetryDelay": "30s",
  "aliveStatusCodes": [200, 206, 403, 429]
}
